extends layout

block content
  h1= title
  - var image_names = ["GreatValue_logo", "bisleri", "kinley", "aqua", "MountValley"]
  - for(let i = 0; i < 3; i++) {
    - let number = Math.floor(Math.random() * 5)
        img(src=`/images/${image_names[number]}.png`, alt="", height="300px", width="300px")
  - }